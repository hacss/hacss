{
  "name": "hacss",
  "version": "2.0.2",
  "description": "Hackable inline style language embedded in HTML classes",
  "repository": "https://github.com/hacss/hacss.git",
  "homepage": "https://hacss.io/",
  "author": "Nick Saunders <nick@saunde.rs>",
  "license": "MIT",
  "engines": {
    "node": ">=8.2.1"
  },
  "main": "index.js",
  "bin": "cli.js",
  "dependencies": {
    "css.escape": "^1.5.1",
    "glob": "^7.1.6",
    "known-css-properties": "^0.18.0",
    "mkdirp": "^0.5.1",
    "ramda": "^0.27.0"
  },
  "scripts": {
    "format": "prettier --trailing-comma all --arrow-parens avoid --parser babel --write .eslintrc.js *.js",
    "lint": "eslint .eslintrc.js *.js test/*.js",
    "test": "node cli.js --output test/styles.css --config test/config.js test/index.html && git diff --exit-code test/styles.css && ./test/output.js"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "prettier": "^2.0.2"
  }
}
