{
  "name": "hacss",
  "version": "0.8.8",
  "description": "Hackable inline style language embedded in HTML classes",
  "repository": "https://github.com/nsaunders/hacss.git",
  "author": "Nick Saunders <nick@saunde.rs>",
  "license": "MIT",
  "engines": {
    "node": ">=10.18.1"
  },
  "main": "index.js",
  "bin": "cli.js",
  "dependencies": {
    "css.escape": "^1.5.1",
    "cssbeautify": "^0.3.1",
    "glob": "^7.1.6",
    "mkdirp": "^0.5.1",
    "postcss": "^7.0.26",
    "postcss-nested": "^4.2.1",
    "ramda": "^0.27.0"
  },
  "scripts": {
    "prepublishOnly": "webpack",
    "format": "prettier --trailing-comma all --parser babel --write .eslintrc.js *.js config/*.js",
    "lint": "eslint .eslintrc.js *.js config/*.js",
    "test": "./cli.js --output test/styles.css --config test/config.js test/index.html && git diff --exit-code test/styles.css && ./test/output.js"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "prettier": "^2.0.2"
  }
}
